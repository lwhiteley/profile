---
import { take } from 'lodash';
import { orderBy } from 'lodash';
import Card from './Card.astro';

export interface Props {
  title: string;
}

const { title } = Astro.props as Props;
---

<Card>
  <div class="space-y-6">
    <h1 class="text-3xl font-bold">{title}</h1>
    <slot />
  </div>
</Card>
